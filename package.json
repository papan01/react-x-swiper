{
  "name": "@papan01yi/react-x-swiper",
  "description": "A react carousel component",
  "version": "1.0.26",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf ./dist ./publish ./node_modules package-lock.json",
    "prod": "npm run build -- --mode=production",
    "dev": "npm run build -- --mode=development",
    "start:prod": "webpack-dev-server --open --mode=production",
    "start:dev": "webpack-dev-server --open --mode=development",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{md,css,scss,yaml,yml}\"",
    "test": "jest",
    "publish:update": "rimraf ./publish && npm run prod && copy package.json publish && copy README.md publish && npm publish ./publish --access public"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "keywords": [
    "React",
    "Swiper",
    "Slider",
    "Carousel "
  ],
  "author": "papan01",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/papan01/react-x-swiper.git"
  },
  "homepage": "https://github.com/papan01/react-x-swiper",
  "bugs": {
    "url": "https://github.com/papan01/react-x-swiper.git/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^3.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.18.2",
    "react-router-dom": "^5.3.1",
    "react-test-renderer": "^16.14.0",
    "sass-loader": "^8.0.2",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.1 || ^16.0.0 || ^18.0.0",
    "react-dom": "^0.14.0 || ^15.0.1 || ^16.0.0 || ^18.0.0"
  }
}
